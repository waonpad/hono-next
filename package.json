{
  "name": "hono-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "lefthook install",
    "setup": "$npm_execpath install && cp -n .env.example .env; exit 0",
    "dev": "next dev",
    "build": "$npm_execpath run db:gen && next build",
    "start": "next start",
    "fmt": "biome format --write .",
    "lnt": "biome lint --write .",
    "che": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest watch --silent",
    "test:ci": "vitest run --silent",
    "db:fmt": "npx prisma format",
    "db:mig": "npx prisma migrate dev",
    "db:mig:prod": "npx prisma migrate deploy",
    "db:gen": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "bun run prisma/seed.ts",
    "type:hono-rpc": "tsc -p tsconfig.hono-rpc.json"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.4.1",
    "@hono/zod-openapi": "^0.16.0",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.19.1",
    "@t3-oss/env-nextjs": "^0.11.0",
    "arctic": "^1.9.2",
    "hono": "^4.6.13",
    "i18next": "^23.15.1",
    "lucia": "^3.2.0",
    "next": "^15.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.4",
    "@biomejs/biome": "^1.8.3",
    "@faker-js/faker": "^9.0.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "happy-dom": "^14.12.3",
    "lefthook": "^1.7.15",
    "postcss": "^8",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.2",
    "vercel": "^37.5.3",
    "vitest": "^2.0.5"
  }
}
